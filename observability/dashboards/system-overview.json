        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "100 * sum(increase(booking_confirmed_total{job=\"booking-api\"}[$__rate_interval])) / clamp_min(sum(increase(booking_confirmed_total{job=\"booking-api\"}[$__rate_interval])) + sum(increase(booking_failed_total{job=\"booking-api\"}[$__rate_interval])), 1)",
          "legendFormat": "Success Rate",
          "refId": "A"
        }
      ],
      "title": "Booking Success Rate",
      "type": "stat"
    },
    {
      "collapsed": false,
