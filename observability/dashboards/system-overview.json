      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_request_duration_seconds_bucket{job=~\"$service\", namespace=~\"$namespace\"}[$__rate_interval])))",
          "legendFormat": "p95 Latency",
          "refId": "A"
        }
      ],
      "title": "Global Latency p95",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
