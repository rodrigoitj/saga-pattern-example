{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Database=FlightDb;Username=postgres;Password=postgres;Port=5432"
  },
  "RabbitMQ": {
    "Host": "rabbitmq",
    "VirtualHost": "/",
    "Username": "guest",
    "Password": "guest"
  },
  "Observability": {
    "ServiceNamespace": "SagaPattern",
    "ServiceVersion": "1.0.0",
    "EnableRuntimeMetrics": true,
    "EnableProcessMetrics": true,
    "Exporters": {
      "Console": false,
      "Prometheus": false,
      "PrometheusScrapeEndpointPath": "/metrics",
      "Otlp": true
    },
    "Otlp": {
      "Endpoint": "http://otel-collector:4318",
      "Protocol": "http/protobuf"
    },
    "Sampling": {
      "TraceIdRatio": 1.0
    }
  }
}