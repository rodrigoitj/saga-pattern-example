{
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=FlightDb;Username=postgres;Password=postgres;Port=5432"
  },
  "RabbitMQ": {
    "Host": "rabbitmq",
    "VirtualHost": "/",
    "Username": "guest",
    "Password": "guest"
  },
  "Observability": {
    "ServiceNamespace": "SagaPattern",
    "ServiceVersion": "1.0.0",
    "EnableRuntimeMetrics": true,
    "EnableProcessMetrics": true,
    "Exporters": {
      "Console": true,
      "Prometheus": true,
      "PrometheusScrapeEndpointPath": "/metrics",
      "Otlp": false
    },
    "Otlp": {
      "Endpoint": "http://otel-collector:4317",
      "Protocol": "grpc"
    },
    "Sampling": {
      "TraceIdRatio": 1.0
    }
  }
}